{
  "manifest_version": 2,
  "name": "Avac",
  "version": "1.0.002",
  "icons": {
    "64": "img/64.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/content.js"
      ],
      "css": [
        "css/popup.css"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "http://SmelayaPandaServer.com/*"
  ],
  "browser_action": {
    "default_title": "Air Article Translator",
    "default_icon": "img/24.png",
    "default_popup": "html/popup.html",
    "options_page": "html/options.html"
  }
}